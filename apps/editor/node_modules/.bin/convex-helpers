#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/olumbe/Repositories/meme-maker/node_modules/.pnpm/convex-helpers@0.1.99_@standard-schema+spec@1.0.0_convex@1.25.2_react@19.1.0__react@19.1.0_typescript@5.8.3_zod@4.0.5/node_modules/convex-helpers/node_modules:/Users/olumbe/Repositories/meme-maker/node_modules/.pnpm/convex-helpers@0.1.99_@standard-schema+spec@1.0.0_convex@1.25.2_react@19.1.0__react@19.1.0_typescript@5.8.3_zod@4.0.5/node_modules:/Users/olumbe/Repositories/meme-maker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/olumbe/Repositories/meme-maker/node_modules/.pnpm/convex-helpers@0.1.99_@standard-schema+spec@1.0.0_convex@1.25.2_react@19.1.0__react@19.1.0_typescript@5.8.3_zod@4.0.5/node_modules/convex-helpers/node_modules:/Users/olumbe/Repositories/meme-maker/node_modules/.pnpm/convex-helpers@0.1.99_@standard-schema+spec@1.0.0_convex@1.25.2_react@19.1.0__react@19.1.0_typescript@5.8.3_zod@4.0.5/node_modules:/Users/olumbe/Repositories/meme-maker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../convex-helpers/bin.cjs" "$@"
else
  exec node  "$basedir/../convex-helpers/bin.cjs" "$@"
fi
